{
  "name": "auto-merge-eip",
  "version": "1.0.0",
  "description": "auto merge eip",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=testing jest __tests__",
    "build": "ncc build ./src/index.ts",
    "watch": "ncc build ./src/index.ts --watch",
    "it": "node -r dotenv/config dist/index.js",
    "prettier-changed": "prettier --write",
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@typescript-eslint/parser": "^4.16.1",
    "@zeit/ncc": "^0.22.3",
    "dotenv": "^8.2.0",
    "front-matter": "^4.0.2",
    "lint-changed": "0.0.0-development",
    "node-fetch": "^2.6.1",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.8",
    "git-format-staged": "^2.1.1",
    "husky": "^5.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-changed"
    }
  },
  "lint-staged": {
    "*.js|*.ts": [
        "npm run prettier-changed"
    ]
}
}
