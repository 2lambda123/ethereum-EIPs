---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Ethereum EIPs - Ethereum Improvement Proposals</title>
    <description>The EIPs listed here are created and merged through the ethereum/EIPs Github repo.</description>
    <link>{{ site.url }}</link>
    <atom:link href="{{ site.url }}/feed.xml" rel="self" type="application/rss+xml" />
    <lastBuildDate>{{ site.time }}</lastBuildDate>
    {% assign eiplist = site.pages | sort: 'eip' | reverse %}
      {% for eip in eiplist %}
      {% if eip.status == "Draft" %}
      {% capture description %}
        {% include eipfeedcontent.html %}
      {% endcapture %}
      <item>
        <title>EIP-{{ eip.eip }}: {{ eip.title | xml_escape }}</title>
        <description>{{ description | xml_escape }}</description>
        <pubDate>{{ eip.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <category>{{ eip.category }}</category>
        <link>{{ site.url }}/{{ eip.url }}</link>
        <guid isPermaLink="true">{{ site.url }}/{{ eip.url }}</guid>
      </item>
      {% endif %}
    {% endfor %}
  </channel>
</rss>
